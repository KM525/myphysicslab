 RowBox[{
  RowBox[{"a", "=", "1"}], ";"}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{"x0", "=", "0"}], ";", 
  RowBox[{"y0", "=", "0"}], ";"}]], "Input"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"g1", "=", 
  RowBox[{"ParametricPlot", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"x0", "+", 
         RowBox[{"a", 
          RowBox[{"(", 
           RowBox[{"t", " ", "-", " ", 
            RowBox[{"Sin", "[", "t", "]"}]}], ")"}]}]}], ",", " ", 
        RowBox[{
         RowBox[{"-", "y0"}], "-", 
         RowBox[{"a", 
          RowBox[{"(", 
           RowBox[{"1", "-", 
            RowBox[{"Cos", "[", "t", "]"}]}], ")"}]}]}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"t", ",", 
        RowBox[{"-", "t"}]}], "}"}]}], "}"}], ",", " ", 
    RowBox[{"{", 
     RowBox[{"t", ",", "0", ",", "Pi"}], "}"}], ",", 
    RowBox[{"AspectRatio", "\[Rule]", "Automatic"}]}], "]"}]}]], "Input"],

